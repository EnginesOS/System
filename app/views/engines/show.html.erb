<table>
<tr>	<% state= @engine.read_state %>
	<td><%=@engine.containerName %></td> 
	<td><%=@engine.fqdn %></td>
	<td><%=@engine.setState %></td>
	<td><%= state %></td>
	
	<% 
		if state == "running"
			%> <td>Stop Pause </td> <%
		elsif state == "paused"
			%> <td>Unpause</td> <%
		elsif start == "stopped" 
			%> <td>Start / Destroy
			</td> <%
		elsif start == "nocontainer" 
			%> <td>Create / Delete </td> 
	<%  end %>
	</tr></table>
